
@{
    ViewBag.Title = "Account List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts
{
   @Scripts.Render("~/bundles/app/acountlist")
}
<h2>Account List</h2>
<table id="lstAccounts" class="display" cellspacing="0" width="100%" data-request-url="@Url.Action("GetAccounts", "Account")">
    <thead>
        <tr>
            <th>Id</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Customer</th>
            <th>Created Date</th>
            <th></th>
        </tr>
    </thead>
   
</table>

